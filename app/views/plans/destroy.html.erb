<%# app/views/plans/destroy.html.slim %>
<%= fields model: @service do |form| %>
  <%= form.fields_for :plans, child_index: params[:index] do |plan_form| %>
    <%= turbo_frame_tag "plan_#{plan_form.index}" do %>
      <%= plan_form.hidden_field :id, value: params[:id] %>
      <%= plan_form.hidden_field :_destroy, value: true %>
    <% end %>
  <% end %>
<% end %>
